{
  "name": "gh-createpr",
  "version": "1.2.4",
  "main": "dist/index.js",
  "bin": {
    "gh-createpr": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "bundle": "esbuild dist/index.js --bundle --platform=node --target=node16 --outfile=dist/index.js --format=cjs --allow-overwrite",
    "start": "node dist/index.js",
    "dev": "ts-node --esm src/index.ts",
    "prepare": "npm run build && chmod +x gh-createpr && chmod +x dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Gonzalo Buasso",
  "license": "ISC",
  "description": "CLI tool to create GitHub PRs from Jira tickets",
  "dependencies": {
    "@types/node": "^24.2.1",
    "commander": "^14.0.0",
    "node-fetch": "^2.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "esbuild": "^0.25.8"
  }
}
